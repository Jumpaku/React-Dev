    "service": "react-dev",
    "workspaceFolder": "/workdir/",
    "extensions": [
        "streetsidesoftware.code-spell-checker"
    ],
    "settings": {
        "editor.formatOnSave": false,
        "editor.wordWrap": "on"
    }
